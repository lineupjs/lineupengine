{
  "name": "lineupengine",
  "description": "a fast engine for rendering large tables consisting of rows, rows+columns, multiple rows+columns instances.",
  "homepage": "https://sgratzl.github.io/lineupengine/",
  "version": "1.1.0-alpha.4",
  "author": {
    "name": "Samuel Gratzl",
    "email": "samuel_gratzl@gmx.at"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sgratzl/lineupengine/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sgratzl/lineupengine.git"
  },
  "main": "build/lineupengine.js",
  "jsnext:main": "build/lineupengine.js",
  "module": "build/lineupengine.js",
  "unpkg": "build/lineupengine.js",
  "types": "src/index.d.ts",
  "sideEffects": [
    "*.scss",
    "*.css"
  ],
  "files": [
    "src/**/*.js",
    "src/**/*.scss",
    "src/**/*.d.ts",
    "src/assets",
    "build/lineupengine.js"
  ],
  "scripts": {
    "clean": "rimraf build dist && npm run clean:compile",
    "clean:compile": "rimraf src/**/*.map src/**/*.js src/**/*.d.ts tests/**/*.js tests/**/*.map tests/**/*.d.ts demo/**/*.js demo/**/*.map demo/**/*.d.ts",
    "compile": "tsc",
    "lint": "tslint --project tsconfig.json -c tslint.json && stylelint src/**/*.scss",
    "docs": "npm run clean:compile && typedoc --out ./build/docs/ src tsd.d.ts",
    "prebuild": "npm run clean && (node -e \"process.exit(process.env.PHOVEA_SKIP_TESTS === undefined?1:0)\" || npm run test)",
    "pretest": "npm run clean && npm run compile",
    "test": "node -e \"process.exit(require('fs').existsSync('./tests')?1:0)\" || karma start",
    "posttest": "npm run lint",
    "test:watch": "karma start --autoWatch=true --singleRun=false",
    "build:dev": "webpack --mode development --devtool source-map",
    "build": "webpack --mode production",
    "watch": "webpack --mode development --watch --devtool source-map",
    "start": "webpack-dev-server --mode development --devtool source-map",
    "preversion": "npm test",
    "prepare": "npm run build",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "release:pre": "npm version prerelease && npm publish --tag=next && git push --follow-tags",
    "predist": "npm run build && npm run docs",
    "dist": "mkdirp dist && cd build && tar cvzf ../dist/lineupengine.tar.gz *"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.7",
    "cache-loader": "^1.2.2",
    "css-loader": "^0.28.11",
    "extract-loader": "^2.0.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "fork-ts-checker-webpack-plugin": "^0.4.1",
    "jasmine": "^3.1.0",
    "jasmine-promises": "^0.4.1",
    "json-loader": "^0.5.7",
    "karma": "^2.0.2",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.2",
    "karma-junit-reporter": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^3.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.9.0",
    "raw-loader": "^1.0.0-beta.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-scss": "^3.1.0",
    "thread-loader": "^1.1.5",
    "ts-loader": "^4.3.0",
    "tslint": "^5.10.0",
    "tslint-consistent-codestyle": "^1.13.0",
    "tslint-eslint-rules": "^5.2.0",
    "typedoc": "^0.11.1",
    "typescript": "^2.8.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.8.3",
    "webpack-cli": "^2.1.3",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {}
}
