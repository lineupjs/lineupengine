{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "target": "es6",
    "outDir": "build/src"
  },
  "typedocOptions": {
    "name": "Lineupengine",
    "out": "./docs",
    "theme": "default",
    "excludeExternals": true,
    "excludePrivate": true,
    "includeVersion": true,
    "hideGenerator": true,
    "exclude": "**/*.(d.ts|js|spec.ts|scss)"
  }
}
